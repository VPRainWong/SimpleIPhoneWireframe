		name="iPhone Wireframe Plugin"
		description="Sample plugin for iPhone Wireframe Plugin"
		provider="Visual Paradigm"
		class="com.vp.plugin.sample.iphonewireframe.IPhoneWireframePlugin">
	
	<actionSets>
